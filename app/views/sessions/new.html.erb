<h1>Sign In</h1>

<form action="<%= session_url %>" method="post">
  <%= auth_form %>

  <label>Email
    <input type="text" name="user[email]" value="">
  </label>
  <br>

  <label>Password
    <input type="password" name="user[password]" value="">
  </label>
  <br>

  <button>Submit</button>
</form>
