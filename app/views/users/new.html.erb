<h1>Sign Up!</h1>

<form action="<%= users_url %>" method="post">
  <%= auth_form %>

  <label>
    <input type="text" name="user[user_name]" value="<%= @user.user_name %>">
  </label>
  <br>

  <label>
    <input type="text" name="user[email]" value="<%= @user.email %>">
  </label>
  <br>

  <label>
    <input type="text" name="user[password]" value="">
  </label>

  <button>Submit</button>
</form>
